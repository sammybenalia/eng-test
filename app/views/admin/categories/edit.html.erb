<div class="row-fluid">
	<div class="span2">
<%= render 'admin/dashboard/navbar'%>	
	</div>
	<div class="span10 topPadding">

<% if @category.errors %>
	<% @category.errors.full_messages.each do |msg|%>
	<%= msg%>
	<%end%>
<%end%>
<%= form_for [:admin, @category] do |c|%>
	<%= c.label :name%>
	<%= c.text_field :name%><br/>
	<%= c.submit "Save"%>
<%end%>
</div>
</div>