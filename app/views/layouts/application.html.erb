<!DOCTYPE html>
<html>
<head>
	<%= display_meta_tags :site => "Untitled Art Blog" %>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
<%= analytics_init :local => Rails.env.production? %>
</head>
<body>

	<%= render 'layouts/social'%>
		<div id="ajaxLoad"></div>
<div class="container">
	
	<a class="siteTitle" href="/"><h1>UNTITLED ART BLOG</h1></a>
	<%= render 'layouts/navigation_bar'%>

<%= yield %>
<%= render 'layouts/footer'%>
</div>
<script type="text/javascript">
/* * * CONFIGURATION VARIABLES: EDIT BEFORE PASTING INTO YOUR WEBPAGE * * */
var disqus_shortname = 'untitledartblog'; // required: replace example with your forum shortname

/* * * DON'T EDIT BELOW THIS LINE * * */
(function () {
    var s = document.createElement('script'); s.async = true;
    s.type = 'text/javascript';
    s.src = '//' + disqus_shortname + '.disqus.com/count.js';
    (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
}());
</script>
</body>
</html>
